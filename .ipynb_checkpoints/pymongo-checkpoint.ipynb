    "from pymongo import MongoClient\n",
    "# Mongo DB 연결\n",
    "conn = MongoClient('127.0.0.1')\n",
    "# 데이터베이스 연결\n",
    "db = conn.mymongo\n",
    "# 컬렉션 연결\n",
    "collection = db.echo\n",
    "# 데이터 가져오기\n",
    "result = collection.find()\n",
    "\n",
    "# 데이터를 다 가져오면 너무 크기 때문에 Cursor형태로 데이터를 가져온다.\n",
    "print(result)"
   ]
